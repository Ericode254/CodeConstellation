{
  "name": "code-constellation",
  "displayName": "Code Constellation",
  "description": "Visualizes your project structure as an interactive force-directed graph, inspired by Obsidian.",
  "publisher": "code-constellation",
  "author": {
    "name": "Ericode254"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ericode254/CodeConstellation.git"
  },
  "icon": "icon.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-constellation.showGraph",
        "title": "Code Constellation: Show Project Graph"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "18.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^8.56.0",
    "glob": "^7.2.3",
    "minimatch": "^5.1.0",
    "mocha": "^11.7.5",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "force-graph": "^1.43.4",
    "ignore": "^7.0.5"
  }
}
